set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set complex_mass_scheme False
import model HC_NLO_X0_UFO-heft
define p = g u c d s u~ c~ d~ s~
define j = g u c d s u~ c~ d~ s~
define l+ = e+ mu+
define l- = e- mu-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
define p = p b b~
define j = j b b~
generate p p > x0 / t [virt=QCD] @0
add process p p > x0 j / t [virt=QCD] @1
add process p p > x0 j j / t [virt=QCD] @2
# added because change from [QCD] to [virt=QCD], which is needed for output standalone
add process p p > x0 j j j / t [virt=QCD] @3
#output ggh012j_5f_NLO_FXFX_125 -nojpeg
output standalone $CMSSW_BASE/src/CMSAachen3B/GeneratorTools/data/ggh_gridpack
